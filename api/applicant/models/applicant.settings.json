{
  "kind": "collectionType",
  "collectionName": "applicants",
  "info": {
    "name": "applicant",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "webs": {
      "collection": "web"
    },
    "title": {
      "type": "string",
      "required": true,
      "minLength": 6
    },
    "email": {
      "type": "email",
      "required": true,
      "minLength": 3
    },
    "phone": {
      "type": "string",
      "required": true,
      "minLength": 9
    },
    "job": {
      "model": "job"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "New",
        "InReview",
        "Interview",
        "Offered",
        "Hired",
        "Reject"
      ],
      "default": "New",
      "private": true
    },
    "content": {
      "type": "richtext",
      "required": true,
      "minLength": 10
    },
    "review": {
      "type": "richtext",
      "private": true
    }
  }
}
